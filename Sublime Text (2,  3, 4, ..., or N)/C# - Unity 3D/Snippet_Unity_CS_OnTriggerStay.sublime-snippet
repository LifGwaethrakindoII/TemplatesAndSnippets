	<![CDATA[/// <summary>Event triggered when this Collider stays with another Collider trigger.</summary>
/// <param name="col">The other Collider involved in this Event.</param>
private void OnTriggerStay(Collider col)
{
	GameObject obj = col.gameObject;

	switch(obj.tag)
	{
		case ${1:GameObjectTag}:
		break;

		default:
		break;
	}
}]]>
